<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_gravity="bottom"
    android:layout_marginBottom="150dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/top_curve_bottomsheet">

        <View
            android:id="@+id/view129"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_max="450dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view205"
            android:layout_width="100dp"
            android:layout_height="5dp"
            android:layout_marginTop="@dimen/dimen_24dp"
            android:layout_marginBottom="@dimen/dimen_24dp"
            android:background="@color/unSelectBg"
            app:layout_constraintEnd_toEndOf="@+id/view129"
            app:layout_constraintStart_toStartOf="@+id/view129"
            app:layout_constraintTop_toTopOf="@+id/view129" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView41"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dimen_24dp"
            android:layout_marginTop="@dimen/dimen_24dp"
            android:layout_marginEnd="@dimen/dimen_24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/view129"
            app:layout_constraintHeight_max="400dp"
            app:layout_constraintStart_toStartOf="@+id/view129"
            app:layout_constraintTop_toBottomOf="@+id/view205"
            tools:itemCount="2"
            tools:listitem="@layout/item_food_cart" />



</androidx.constraintlayout.widget.ConstraintLayout>