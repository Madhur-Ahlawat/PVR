<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".ui.watchList.WatchListActivity">

    <View
        android:id="@+id/view175"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/include17"
        layout="@layout/toolbar"
        android:layout_width="0dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView281"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:textSize="@dimen/dimen_13sp"
        android:textColor="@color/h8Point1Color"
        android:text="@string/we_ll_notify"
        app:layout_constraintEnd_toEndOf="@+id/view175"
        app:layout_constraintStart_toStartOf="@+id/view175"
        app:layout_constraintTop_toBottomOf="@+id/include17" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView53"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="30dp"
        tools:listitem="@layout/watchlist_item"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView281"
        app:layout_constraintStart_toStartOf="@+id/textView281"
        app:layout_constraintTop_toBottomOf="@+id/textView281" />


</androidx.constraintlayout.widget.ConstraintLayout>