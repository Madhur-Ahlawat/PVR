<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="@dimen/padding_10"
        android:layout_marginBottom="@dimen/dimen_12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/view86"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <de.hdodenhof.circleimageview.CircleImageView
            app:layout_constraintEnd_toEndOf="@+id/view86"
            app:layout_constraintStart_toStartOf="@+id/view86"
            app:layout_constraintTop_toTopOf="@+id/view86"
            android:id="@+id/imageView66"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:padding="@dimen/dimen_15dp"
            android:background="@drawable/border_circle_gray"
            android:src="@drawable/food_all" />


        <TextView
            android:id="@+id/textView136"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/sf_pro_text_regular"
            android:textColor="@color/black"
            android:includeFontPadding="false"
            android:textAlignment="center"
            android:textSize="@dimen/dimen_14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView66"
            app:layout_constraintStart_toStartOf="@+id/imageView66"
            app:layout_constraintTop_toBottomOf="@+id/imageView66"
            tools:text="Combo" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>