<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout50"
        android:layout_width="@dimen/dimen_70dp"
        android:layout_height="@dimen/dimen_60dp"
        android:background="@drawable/experience_item_un_select"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/view121"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView88"
            android:layout_width="@dimen/dimen_40dp"
            android:layout_height="@dimen/dimen_40dp"
            app:layout_constraintBottom_toBottomOf="@+id/view121"
            app:layout_constraintEnd_toEndOf="@+id/view121"
            app:layout_constraintStart_toStartOf="@+id/view121"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription"
            tools:srcCompat="@tools:sample/avatars" />


    </androidx.constraintlayout.widget.ConstraintLayout>


    <ImageView
        android:id="@+id/imageView89"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout50"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout50"
        app:srcCompat="@drawable/ok_yellow"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView181"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="40dp"
        android:fontFamily="@font/montserrat_regular"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="@dimen/dimen_12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout50"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout50"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout50"
        tools:text="Seat  Comfort" />
</androidx.constraintlayout.widget.ConstraintLayout>